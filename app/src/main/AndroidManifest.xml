<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fire.deekshithrajbasa.com.instagramhashtags">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Splash">
        <activity
            android:screenOrientation="userPortrait" android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".instahashtags"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".category"
            android:screenOrientation="sensorPortrait"
            android:theme="@style/AppTheme" />
        <activity
            android:screenOrientation="userPortrait"
            android:name=".fbhashtags"
            android:theme="@style/AppTheme" />
        <activity
            android:screenOrientation="userPortrait"
            android:name=".customview"
            android:theme="@style/AppTheme" />
        <activity
            android:screenOrientation="userPortrait"
            android:name=".fabmenu"
            android:theme="@style/Diloge" />
        <!--
            <activity
            android:name=".MainActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/AppTheme" />
        -->
        <activity
            android:screenOrientation="userPortrait"
            android:name=".feed"
            android:label="@string/title_activity_feed"
            android:theme="@style/AppTheme" />
        <activity android:name=".nointernet"></activity>
    </application>

</manifest>